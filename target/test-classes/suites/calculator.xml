<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Second suite" verbose="1" parallel="methods" thread-count="2">
    <test name="Sum">
        <classes>
            <class name="com.epam.module4_1.SumWholeCalculatorTest"></class>
            <class name="com.epam.module4_1.SumFracCalculatorTest"></class>
        </classes>
    </test>
    <test name="Subtraction">
        <classes>
            <class name="com.epam.module4_1.SubWholeCalculatorTest"></class>
            <class name="com.epam.module4_1.SubFracCalculatorTest"></class>
        </classes>
    </test>
    <test name="Multiplication">
        <classes>
            <class name="com.epam.module4_1.MultWholeCalculatorTest"></class>
            <class name="com.epam.module4_1.MultFracCalculatorTest"></class>
        </classes>
    </test>
    <test name="Division">
        <classes>
            <class name="com.epam.module4_1.DivWholeCalculatorTest"></class>
            <class name="com.epam.module4_1.DivFracCalculatorTest"></class>
        </classes>
    </test>
    <test name="Sqrt(0)">
        <parameter name="a" value="0"/>
        <parameter name="expected" value="0"/>
        <classes>
            <class name="com.epam.module4_1.SqrtCalculatorTest"></class>
        </classes>
    </test>
    <test name="Sqrt(9)">
        <parameter name="a" value="9"/>
        <parameter name="expected" value="3"/>
        <classes>
            <class name="com.epam.module4_1.SqrtCalculatorTest"></class>
        </classes>
    </test>
    <test name="Sqrt(1)">
        <groups>
            <run>
                <exclude name="one"/>
            </run>
        </groups>
        <classes>
            <class name="com.epam.module4_1.SqrtCalculatorTest"></class>
        </classes>
    </test>
    <test name="Sin">
        <classes>
            <class name="com.epam.module4_1.SinCalculatorTestSimple"/>
        </classes>
    </test>
</suite>