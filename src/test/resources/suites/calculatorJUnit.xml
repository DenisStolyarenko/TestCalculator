<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Second suite" verbose="1" parallel="methods" thread-count="2">
    <test name="Sum">
        <classes>
            <class name="com.epam.moduleJUnit.SubWholeCalculatorTest"></class>
            <class name="com.epam.moduleJUnit.SumFracCalculatorTest"></class>
        </classes>
    </test>
    <test name="Subtraction">
        <classes>
            <class name="com.epam.moduleJUnit.SubWholeCalculatorTest"></class>
            <class name="com.epam.moduleJUnit.SubFracCalculatorTest"></class>
        </classes>
    </test>
    <test name="Multiplication">
        <classes>
            <class name="com.epam.moduleJUnit.MultWholeCalculatorTest"></class>
            <class name="com.epam.moduleJUnit.MultFracCalculatorTest"></class>
        </classes>
    </test>
    <test name="Division">
        <classes>
            <class name="com.epam.moduleJUnit.DivWholeCalculatorTest"></class>
            <class name="com.epam.moduleJUnit.DivFracCalculatorTest"></class>
        </classes>
    </test>
    <test name="Sqrt">
        <groups>
            <run>
                <exclude name="one"/>
            </run>
        </groups>
        <classes>
            <class name="com.epam.moduleJUnit.SqrtCalculatorTest"></class>
        </classes>
    </test>
    <test name="Sin">
        <classes>
            <class name="com.epam.moduleJUnit.SinCalculatorTest"/>
        </classes>
    </test>
    <test name="Cos">
        <classes>
            <class name="com.epam.moduleJUnit.CosCalculatorTest"/>
        </classes>
    </test>
    <test name="Ctg">
        <classes>
            <class name="com.epam.moduleJUnit.CtgCalculatorTest"/>
        </classes>
    </test>
    <test name="Tg">
        <classes>
            <class name="com.epam.moduleJUnit.TgCalculatorTest"/>
        </classes>
    </test>
    <test name="checkNegative">
        <classes>
            <class name="com.epam.moduleJUnit.isNegativeCalculatorTest"/>
        </classes>
    </test>
    <test name="checkPositive">
        <classes>
            <class name="com.epam.moduleJUnit.isPositiveCalculatorTest"/>
        </classes>
    </test>
    <test name="Pow1">
        <classes>
            <class name="com.epam.moduleJUnit.PowCalculatorTest"/>
        </classes>
    </test>
</suite>